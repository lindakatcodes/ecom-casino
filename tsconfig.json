{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@api/*": ["src/pages/api/*"],
      "@icons/*": ["src/icons/*"],
      "@vars/*": ["src/constants.ts"]
    }
  }
}